


















SELECT concat(link,'>',hyperlink) ,sysdate() FROM tbl_adinventory s JOIN tbl_finallist c
ON FIND_IN_SET(LOWER(s.keyword),LOWER(c.splitted_Column)) where c.id='NFL'

insert into tbl_adinventory values("Oakland","oakland.athletics.mlb.com/ticketing/?c_id=oak","Oakland Athletics Tickets","Y");
insert into tbl_adinventory values("moneyball","https://www.verizon.com/Ondemand/Movies/MovieDetails/Moneyball/TVNX0011284101153930","Moneyball on demand video","Y");
insert into tbl_adinventory values("Cricket","www.cricbuzz.com","Live Cricket Scores","Y");
insert into tbl_adinventory values("Shopping","www.amazon.com","Shopping at Amazon","Y")

http://www.google.com/search?q=stackoverflow&btnI


SELECT concat(link,'>',hyperlink)  ,sysdate() , lower(a.keyword),lower(b.splitted_Column)
FROM tbl_adinventory a, tbl_finallist b
where 1 = 1
and replace(cast(rtrim(ltrim(lower(a.keyword))) as CHAR(100)),' ','') =replace(cast(rtrim(ltrim(lower(b.splitted_Column))) as CHAR(100)),' ','')
and b.id='NFL'


SELECT concat(link,'>',hyperlink)  ,sysdate() , lower(a.keyword),lower(b.splitted_Column), lower(a.keyword)=lower(b.splitted_Column),
lower(b.splitted_Column)='cricket',lower(b.splitted_Column)='cricket', Strcmp(lower(a.keyword), lower(b.splitted_Column))

FROM tbl_adinventory a, tbl_finallist b
where 1 = 1

and b.id='NFL'

select * from tbl_adinventory where LOWER(keyword) in (select LOWER(splitted_Column) from tbl_finallist where id ='NFL')


SELECT * FROM `tbl_adinventory`
JOIN `tbl_finallist` ON `LOWER(keyword)` = `LOWER(splitted_Column)`
WHERE `tbl_finallist`.`id` = 'NFL'

select * from tbl_adinventory
join tbl_finallist on tbl_adinventory.keyword=tbl_finallist.splitted_Column WHERE tbl_finallist.id = 'NFL'


